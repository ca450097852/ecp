/* CSS Document */

* {
	margin: 0;
	border: 0;
	padding: 0;
}
.cl {
	clear: both
}
ul {
	list-style-type: none;
}
a {
	text-decoration: none;
	color: #333;
}
.title {
	line-height: 24px;
	font-size: 18px;
	overflow: hidden;
	font-weight: bold;
	margin-bottom: 5px;
}
.title-01 {
	height: 35px;
	overflow: hidden;
}
.btn {
	height: 26px;
	padding: 0 10px;
	text-align: center;
	line-height: 26px;
	background: #ff6c00;
	color: #fff;
	font-weight: bold;
	-moz-border-radius: 20px; /* Firefox */
	-webkit-border-radius: 20px; /* Safari 和 Chrome */
	border-radius: 20px; /* Opera 10.5+, 以及使用了IE-CSS3的IE浏览器 */
}
.bd-r3 {
	-moz-border-radius: 3px; /* Firefox */
	-webkit-border-radius: 3px; /* Safari 和 Chrome */
	border-radius: 3px; /* Opera 10.5+, 以及使用了IE-CSS3的IE浏览器 */
	overflow: hidden;
}
strong.price {
	color: #ff6c00;
	display: inline-block;
	font-size: 18px;
}
strong.price span {
	font-size: 14px;
	font-weight: normal;
}
.old-price {
	height: 24px;
	text-decoration: line-through;
	color: #999;
	display: inline-block;
	padding: 5px;
	font-style: normal;
}
.pre {
	width: 5px;
	height: 10px;
	display: block;
	background: url(images/pre.png) no-repeat;
}
.next {
	width: 5px;
	height: 10px;
	display: block;
	background: url(images/next.png) no-repeat;
}
.result {
	margin: 25px 0;
}
.icon-tips {
	float: left;
}
.icon-tips i {
	height: 18px;
	line-height: 18px;
	padding: 0 3px;
	margin-right: 5px;
	background: #ff6c00;
	color: #fff;
	font-style: normal;
	display: inlie-block;
	float: left;
}
.page-bottom{
	height:30px;
	margin:20px 0;
	float:right}
.page-bottom-content{
	font-size: 12px;
	color:#ff6500;
	text-align:center;
	float:right;
}
.page-bottom a{
	border:#ff9600 1px solid;
	padding:5px 10px;
	color: #ff6500;
	margin-right: 3px;
}
.page-bottom a:hover {
	border: #ff9600 1px solid;
	color: #ff6500;
	background-color: #ffc794;
}
.page-bottom a:active {
	border: #ff9600 1px solid;
	background-image: none;
	color: #ff6500;
	background-color: #ffc794;
}
.page-bottom span.current {
	padding:5px 10px;
	border: #ff6500 1px solid;
	font-weight: bold;
	color: #ff6500;
	margin-right: 3px;
	background-color: #ffbe94;
}
.page-bottom span.disabled {
	border: #ffe3c6 1px solid;
	padding:5px 10px;
	color: #ffe3c6; margin-right: 3px;
}

