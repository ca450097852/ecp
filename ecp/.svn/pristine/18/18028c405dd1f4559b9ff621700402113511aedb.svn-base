/*pay.html*/
.al-container{
	width:100%;
	position:absolute;
	min-width:950px;
	}
.al-head{
	height: 50px;
	background-color: #fff;
	border-bottom: 1px solid #d7d8d8;
	box-shadow: 0 1px 1px #ddd;
	margin-bottom: 1px;
}
.al-head-content{
	width:950px;
	margin:0 auto;
	}
.al-logo{
	width:100px;
	height:30px;
	padding-top:10px;
	float:left;
	}
.al-title{
	float:left;
	border-left:1px solid #333;
	font-size:18px;
	margin:10px 0;
	padding:0 5px;
	}
.al-head-content form{
	width:120px;
	float:left;}
.al-head-content select{
	width:100px;
	margin-top:15px;
	margin-left:10px;
	display: inline-block;
	*display: block;
	*width: 101px;
	padding: 1px 5px;
	color: #5b6a6f;
	text-align: center;
	background-color: #eff0f1;
	font-size:14px;
	}
.al-head-content ul{
	float:right;
	margin-top:20px;
	}
.al-head-content ul li{
	float:left;
	padding-left:5px;
	margin-left:5px;}
.al-head-content ul li.al-account b{
	font-weight:normal;
	}
.al-head-content ul li:last-child{
	border-left:1px solid #333;
	}
.al-main{
	width:950px;
	margin:0 auto;
	}
.al-order{
	margin-top:15px;
	padding-bottom:15px;
	border-bottom: 3px solid #a6a6a6;
	position:relative;}
.al-order .al-qr{
	float:left;
	margin-right:15px;
	}

.al-order .al-order-title{
	width:620px;
	line-height:25px;
	padding-top:15px;
	
	}
.al-order .al-order-seller{
	width:620px;
	line-height:25px;
	padding-top:15px;
	}
.al-order .al-order-amount{
	float:right;
	position:absolute;
	top:30px;
	right:10px;
	}
.al-order .al-detail{
	position: absolute;
right: 40px;
bottom: 0;
line-height: 18px;
padding: 2px 8px 1px;
font-weight: 400;
border-top: none;
background-color: #b3b3b3;
color:#fff;}
.al-order .al-order-amount em{
	color: #ff8208;
font-weight: 700;
font-size: 22px;
font-style:normal;
	}
	
.al-main-content{
	background:#fff;
	padding-bottom:20px;
	}
.al-main-content ul{
	width:910px;
	margin:20px;}
