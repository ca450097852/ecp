.wwtable {
	background: #c4c5c9 none repeat scroll 0 0;
	border: 0 none;
	margin: 10px 0 0 10px;
	width: 98%;
}

.wwtable input { /*border: 0 none;*/
	height: 20px;
	width: 200px;
}

.wwtable tr {
	background: #ffffff none repeat scroll 0 0;
}

.wwtable .fmhead {
	font-size: 14px;
	font-weight: bold;
}

.table_name {
	background: #e3e8ec none repeat scroll 0 0;
	font-size: 14px;
	height: 30px;
	text-align: right;
	width: 250px;
}

td {
	border-bottom: 1px solid #ebebeb;
}

.txtinput {
	width: 180px;
}

textarea,input {
	font-family: "宋体";
	font-size: 12px;
}

.buttom {
	margin: 0 auto;
	padding-top: 20px;
	width: 500px;
	text-align: center;
}

.buttom input {
	background: rgba(0, 0, 0, 0) url("../../../images/web/comm/buttom.jpg")
		repeat-x scroll 0 0;
	border: 1px solid #54647a;
	color: #fff;
	font-weight: bold;
	height: 22px;
	padding: 0 5px;
}

.uploadify {
	margin: 0 auto 0 8px;
}

.preview {
	width: 80px;
	height: 20px;
	margin: 5px auto 0 8px;
	cursor: pointer;
}

.ecp_a:link {
	text-decoration: none;
	color: white;
}

.ecp_a:visited {
	text-decoration: none;
	color: white;
}

.ecp_a:hover {
	text-decoration: none;
	color: white;
}

.ecp_a:active {
	text-decoration: none;
	color: white;
}

.brandItem {
	width: 100px;
	height: 100px;
	margin: 10px;
	float: left;
	text-align: center;
	cursor: pointer;
	overflow: hidden;
}

.brandItem img {
	width: 100%;
	display: block;
}

.brandItem span {
	width: 100%;
	overflow: hidden;
}

.select {
	height: 100%;
	position: relative;
	text-align: center;
	top: -100%;
	width: 100%;
	background-color: #d8d8d8;
	filter: alpha(opacity = 50);
	-moz-opacity: 0.5;
	-khtml-opacity: 0.5;
	opacity: 0.5;
	display: none;
}

.button_div{
	bottom: 10px;
    position: absolute;
    text-align: center;
    width: 100%;
}

#modelFile{display: inline;float: left;}