.zhifu1{width:1048px;height:100%;float:left;padding:20px 50px;background:#f5f6fb;margin-left:20px;line-height:30px;}
.zhifu1 a{font-size:12px;}
.zhifu2{width:1044px;height:100%;float:left;padding:20px 50px;background:#fff;margin-left:20px;line-height:30px;border:2px solid #bcccee;}
.zhifu1 a{font-size:12px;}
.zhifu3{width:1048px;height:100%;float:left;margin-left:20px;font-weight:bold;line-height:25px;}

/*评价*/
.line{width:950px;height:110px;}
.shopdetail {
position: relative;
color: #404040;
padding: 18px 0;
background-image: url(../../../images/web/comm/T1zZF.XoNzXXXXXXXX-16-258.png);
background-repeat: no-repeat;
background-position: 0 -18px;
background-color: #F1F5F8;
height: 72px;
border: 1px solid #C4D5DF;}


.shopdetail .hd {
position: absolute;
left: 0;
top: 0;
height: 110px;
width: 39px;
line-height: 1.2em;
background-image: url(../../../images/web/comm/T1zZF.XoNzXXXXXXXX-16-258.png);
background-repeat: no-repeat;
background-position: 24px -159px;}
.shopdetail .bd {
padding-left: 100px;
margin-left: 39px;
}
