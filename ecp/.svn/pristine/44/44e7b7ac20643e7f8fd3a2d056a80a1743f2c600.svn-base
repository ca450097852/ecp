.miaoshu{display:block;height:125px; min-height:125px;overflow:hidden;float:left;}

/*********首页底部帮助导航样式开始************/
.bottom{
	width:100%;
	margin-top:15px;
	padding-bottom:20px;
	/*background:url(../../../images/web/comm/bottom.gif) repeat-x;*/
	}
.bottom_help{
	width:165px;
	float:left;
	padding:20px 0 0 20px;
	}
.bottom_help .bottom_t{
	font-family:"微软雅黑";
	font-size:18px;
	height:32px;
	padding-left:40px;
	}
.bottom_help li{
	line-height:25px;
	padding-left:40px;
	}	
.way{
	height:32px;
	background:url(../../../images/web/comm/bottom_help.gif) no-repeat;
	}
.security{
	height:32px;
	background:url(../../../images/web/comm/bottom_help.gif) no-repeat;
	background-position:0 -32px;
	}
.after_sales{
	height:32px;
	background:url(../../../images/web/comm/bottom_help.gif) no-repeat;
	background-position:0 -64px;
	}
.about{
	height:32px;
	background:url(../../../images/web/comm/bottom_help.gif) no-repeat;
	background-position:0 -96px;
	}
.copyright{
	border-top:1px solid #d5d5d5;
	text-align:center;
	line-height:30px;
	padding-top:10px;
	}
.copyright_about a{
	color:#3366cc;
	padding:0 10px;
	border-right:1px solid #d4d4d4;
	}
.copyright_about a:hover{
	color:#3366cc;
	text-decoration:underline;
	}			
	
/*首页底部帮组导航样式开始*/

#footHelp {width:1200px;margin:5px 0 0 0 ;padding:5px 0;background:#73B2DD;text-align:center;float:left}
#footHelp li {display:inline;margin:0 10px;}
#footHelp li a {font-size:16px;font-weight:bold;color:#fff;padding-left:22px;}
#footHelp li .f_1 {background:url(../../../images/web/comm/ftico.png) no-repeat left top;}
#footHelp li .f_2 {background:url(../../../images/web/comm/ftico.png) no-repeat left -25px;}
#footHelp li .f_3 {background:url(../../../images/web/comm/ftico.png) no-repeat left -50px;}
#footHelp li .f_4 {background:url(../../../images/web/comm/ftico.png) no-repeat left -75px;}
#ft {width:960px;margin:0 auto;;background:#fff ;color:#0575C2;}
#ft .copyright {padding:10px 0;line-height:20px;text-align:center;margin:0 auto;}



	
	
	
