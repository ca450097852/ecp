.wwtable {
	background: #c4c5c9 none repeat scroll 0 0;
	border: 0 none;
	margin: 10px 0 0 10px;
	width: 98%;
}

.wwtable input { 
display: block;
width: 100%;
padding: 0.5em;
line-height: 1.2;
color: #555555;
vertical-align: middle;
background-color: #fff;
border-radius: 0;
border: 1px solid #E4EAEE;
transition: border-color 0.15s;
}

.wwtable tr {
	background: #ffffff none repeat scroll 0 0;
	height: 50px;
}



.table_name {
	text-align:center;
	vertical-align:middle;
	padding-top:0;
	font-size: 14px;
	font-weight: 600;
	width: 110px;
}


.txtinput {
	width: 180px;
}

textarea,input {
	font-family: "宋体";
	font-size: 12px;
}

.buttom {
	margin: 0 auto;
	padding-top: 20px;
	width: 500px;
	text-align: center;
}

.buttom input {
	background:url("../../images/goods/buttom.jpg")
		repeat-x scroll 0 0;
	border: 1px solid #54647a;
	color: #fff;
	font-weight: bold;
	height: 22px;
	padding: 0 5px;
}

.uploadify {
	margin: 0 auto 0 8px;
}

.preview {
	width: 80px;
	height: 20px;
	margin: 5px auto 0 8px;
	cursor: pointer;
}

.ecp_a:link {
	text-decoration: none;
	color: white;
}

.ecp_a:visited {
	text-decoration: none;
	color: white;
}

.ecp_a:hover {
	text-decoration: none;
	color: white;
}

.ecp_a:active {
	text-decoration: none;
	color: white;
}

.brandItem {
	width: 100px;
	height: 120px;
	margin: 10px;
	float: left;
	text-align: center;
	cursor: pointer;
	overflow: hidden;
}

.brandItem img {
	width: 100%;
	display: block;
}

.brandItem span {
	width: 100%;
	overflow: hidden;
}

.select {
	height: 100%;
	position: relative;
	text-align: center;
	top: -100%;
	width: 100%;
	background-color: #d8d8d8;
	filter: alpha(opacity = 50);
	-moz-opacity: 0.5;
	-khtml-opacity: 0.5;
	opacity: 0.5;
	display: none;
}

.button_div{
	bottom: 10px;
    position: absolute;
    text-align: center;
    width: 100%;
}

#modelFile{display: inline;float: left;}




.product-info .line {
    width: 450px;
    min-height: 36px;
    line-height: 36px;
    margin-bottom: 5px;
}

.goods-type .goods-type-box {
    width: 100%;
}

.goods-normal{
    height: 24px;
    padding: 0 12px;
    margin-right: 5px;
    line-height: 24px;
    display: inline-block;
    border: #d0d0d0 1px solid;
}

.cancel{
	width:12px;
	height:12px;
	display:block;
	background:url(../../../seller/images/icon-cancel.png);
	position:absolute;
	top:-5px;
	right:-5px;
}























